<?xml version="1.0"?>
<includes>
	<!-- Common -->
	<include name="PhotoThumbCard">
		<control type="image">
			<posx>13</posx>
			<posy>130</posy>
			<width>122</width>
			<height>34</height>
			<include>Card</include>
			<animation effect="fade" start="0" end="40" time="0" condition="true">Conditional</animation>
		</control>
	</include>
	<include name="SelectedBorder">
		<control type="image">
			<posx>11</posx>
			<posy>11</posy>
			<width>128</width>
			<height>155</height>
			<include>Border</include>
		</control>
	</include>
	<include name="PhotoThumb">
		<control type="group">
			<visible>ListItem.Type(photo)</visible>
			<control type="image">
				<posx>0</posx>
				<posy>0</posy>
				<width>148</width>
				<height>148</height>
				<include>Shadow</include>
			</control>
			<control type="image">
				<description>Poster image</description>
				<posx>13</posx>
				<posy>13</posy>
				<width>122</width>
				<height>122</height>
				<texture background="true">default-square-small.png</texture>
			</control>
			<control type="image">
				<description>Poster image</description>
				<posx>13</posx>
				<posy>13</posy>
				<width>122</width>
				<height>122</height>
				<texture background="true" diffuse="mask-square.png">$INFO[ListItem.Art(thumb)]</texture>
				<aspectratio scalediffuse="false" align="center" aligny="top">scale</aspectratio>
			</control>
		</control>
	</include>
	<include name="_PhotoAlbumCover">
		<control type="group">
			<visible>ListItem.Type(photoAlbum)</visible>
			<control type="image">
				<posx>0</posx>
				<posy>0</posy>
				<width>148</width>
				<height>148</height>
				<include>Shadow</include>
			</control>
			<control type="image">
				<description>Poster image</description>
				<posx>13</posx>
				<posy>13</posy>
				<width>122</width>
				<height>122</height>
				<texture background="true">default-square-small.png</texture>
			</control>
			<control type="image">
				<description>Poster image</description>
				<posx>13</posx>
				<posy>13</posy>
				<width>122</width>
				<height>122</height>
				<texture background="true" diffuse="mask-square.png">$INFO[ListItem.Art(thumb)]</texture>
				<aspectratio scalediffuse="false" align="center" aligny="top">scale</aspectratio>
				<animation effect="fade" start="0" end="60" time="0" condition="true">Conditional</animation>
			</control>
			<!-- base frame -->
			<control type="image">
				<posx>13</posx>
				<posy>13</posy>
				<width>122</width>
				<height>122</height>
				<texture background="true">mask-photoalbum-base.png</texture>
			</control>
			<!-- thumb -->
			<control type="image">
				<description>Poster image</description>
				<posx>46</posx>
				<posy>39</posy>
				<width>72</width>
				<height>70</height>
				<texture background="true" diffuse="mask-photoalbum-center-photo.png">$INFO[ListItem.Art(thumb)]</texture>
				<aspectratio scalediffuse="false" align="center" aligny="top">scale</aspectratio>
			</control>
		</control>
	</include>
	<include name="PhotoAlbumCover">
		<control type="group">
			<visible>ListItem.Type(photoAlbum)</visible>
			<control type="image">
				<posx>0</posx>
				<posy>0</posy>
				<width>148</width>
				<height>148</height>
				<include>Shadow</include>
			</control>
			<control type="image">
				<description>Poster image</description>
				<posx>13</posx>
				<posy>13</posy>
				<width>122</width>
				<height>122</height>
				<texture background="true">default-square-small.png</texture>
			</control>
			<control type="image">
				<description>Poster image</description>
				<posx>13</posx>
				<posy>13</posy>
				<width>122</width>
				<height>122</height>
				<texture background="true" diffuse="mask-square.png">$INFO[ListItem.Art(thumb)]</texture>
				<aspectratio scalediffuse="false" align="center" aligny="top">scale</aspectratio>
			</control>
			<control type="image">
				<posx>13</posx>
				<posy>97</posy>
				<width>122</width>
				<height>39</height>
				<include>Black</include>
				<animation effect="fade" start="0" end="40" time="0" condition="true">Conditional</animation>
			</control>
		</control>
	</include>
	<include name="PhotoLabel">
		<posy>141</posy>
		<posx>76</posx>
		<width>110</width>
		<height>20</height>
		<align>center</align>
		<aligny>center</aligny>
		<label>$INFO[ListItem.Label]</label>
		<font>Regular-14</font>
		<shadowcolor>ff222222</shadowcolor>
	</include>
	<!-- LibraryPhoto -->
	<include name="PlotBGLibrary">
		<control type="image">
			<posx>0</posx>
			<posy>0</posy>
			<width>556</width>
			<height>720</height>
			<include>Black</include>
			<animation effect="fade" start="0" end="20" time="0" condition="true">Conditional</animation>
			<visible>true</visible>
		</control>
		<control type="group">
			<control type="image">
				<posy>385</posy>
				<posx>0</posx>
				<width>556</width>
				<height>263</height>
				<include>Black</include>
				<animation effect="fade" start="0" end="50" time="0" condition="true">Conditional</animation>
			</control>
			<control type="image">
				<posx>556</posx>
				<posy>500</posy>
				<include>ArrowBlack</include>
				<animation effect="fade" start="0" end="60" time="0" condition="true">Conditional</animation>
			</control>
		</control>
	</include>
	<include name="AlbumTitleLibrary">
		<control type="group">
			<posy>34</posy>
			<posx>100</posx>
			<control type="label">
				<posy>0</posy>
				<posx>0</posx>
				<width>440</width>
				<height>20</height>
				<align>left</align>
				<aligny>center</aligny>
				<label>$INFO[Container.FirstTitle]</label>
				<font>Bold-36</font>
				<textcolor>white</textcolor>
				<shadowcolor>ff222222</shadowcolor>
			</control>
			<control type="label">
				<posy>0</posy>
				<posx>440</posx>
				<width>440</width>
				<height>20</height>
				<align>right</align>
				<aligny>center</aligny>
				<label> 2011</label>
				<font>Bold-36</font>
				<textcolor>white</textcolor>
				<shadowcolor>ff222222</shadowcolor>
			</control>
		</control>
	</include>
	<include name="AlbumIconsLibrary">
		<control type="group">
			<posx>0</posx>
			<posy>90</posy>
			<control type="group">
				<posx>252</posx>
				<posy>-12</posy>
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>286</width>
					<height>286</height>
					<include>Shadow</include>
				</control>
				<control type="image">
					<description>Poster image</description>
					<posx>13</posx>
					<posy>13</posy>
					<width>260</width>
					<height>260</height>
					<texture background="true" diffuse="mask-square.png">$INFO[ListItem.Property(Fanart_Image)]</texture>
					<aspectratio scalediffuse="false" align="center" aligny="top">scale</aspectratio>
				</control>
			</control>
		</control>
	</include>
	<include name="PlotLibrary">
		<control type="group">
			<!-- album plot -->
			<control type="textbox">
				<posx>100</posx>
				<posy>410</posy>
				<width>430</width>
				<height>196</height>
				<include>PlotFormat</include>
				<label>$INFO[Container.Property(summary)]</label>
			</control>
			<control type="textbox">
				<posx>100</posx>
				<posy>410</posy>
				<width>430</width>
				<height>196</height>
				<include>PlotFormat</include>
				<include>NoPlotDefault</include>
				<visible>IsEmpty(Container.Property(summary))</visible>
			</control>
		</control>
	</include>
	<!-- Library -->
	<include name="LibraryPhoto">
		<control type="group">
			<visible>!Container.PlexContent(PhotoAlbums) + !Container.PlexContent(Channel)</visible>
			<include>FadeDialog</include>
			<include>rightSideSlide</include>
			<!-- main list -->
			<control type="group" id="777">
				<control type="panel" id="51">
					<visible>!Container.PlexContent(PhotoAlbums) + !Container.PlexContent(Channel)</visible>
					<viewtype label="LibraryPhoto">panel</viewtype>
					<onup>51</onup>
					<ondown>51</ondown>
					<onright>Container.NextViewMode</onright>
					<onleft>998</onleft>
					<orientation>vertical</orientation>
					<posx>93</posx>
					<posy>15</posy>
					<width>1200</width>
					<height>690</height>
					<itemlayout width="146" height="171">
						<include>PhotoThumbCard</include>
						<include>PhotoThumb</include>
						<include>PhotoAlbumCover</include>
						<control type="image">
							<posx>100</posx>
							<posy>101</posy>
							<width>30</width>
							<height>30</height>
							<texture>icon-photoalbum.png</texture>
							<visible>ListItem.Type(photoAlbum)</visible>
						</control>
						<control type="label">
							<include>PhotoLabel</include>
							<textcolor>white</textcolor>
						</control>
					</itemlayout>
					<focusedlayout width="146" height="171">
						<include>PhotoThumbCard</include>
						<include>PhotoThumb</include>
						<include>PhotoAlbumCover</include>
						<control type="image">
							<posx>100</posx>
							<posy>101</posy>
							<width>30</width>
							<height>30</height>
							<texture>icon-photoalbum.png</texture>
							<colordiffuse>orange</colordiffuse>
							<visible>ListItem.Type(photoAlbum)</visible>
						</control>
						<control type="label">
							<include>PhotoLabel</include>
							<textcolor>orangeLight</textcolor>
						</control>
						<include>SelectedBorder</include>
					</focusedlayout>
				</control>
			</control>
		</control>
	</include>
	<!-- Album -->
	<include name="AlbumPhoto">
		<control type="group">
			<visible>Container.PlexContent(PhotoAlbums)</visible>
			<include>FadeDialog</include>
			<include>rightSideSlide</include>
			<control type="group">
				<visible> Control.IsVisible(52)</visible>
				<include>PlotBGLibrary</include>
				<include>AlbumTitleLibrary</include>
				<include>AlbumIconsLibrary</include>
				<include>PlotLibrary</include>
			</control>
			<!-- main list -->
			<control type="group" id="777">
				<control type="panel" id="52">
					<visible>Container.PlexContent(PhotoAlbums)</visible>
					<viewtype label="AlbumPhoto">panel</viewtype>
					<onup>51</onup>
					<ondown>51</ondown>
					<onright>Container.NextViewMode</onright>
					<onleft>998</onleft>
					<orientation>vertical</orientation>
					<posx>573</posx>
					<posy>15</posy>
					<width>685</width>
					<height>690</height>
					<itemlayout width="136" height="171">
						<include>PhotoThumbCard</include>
						<include>PhotoThumb</include>
						<include>PhotoAlbumCover</include>
						<control type="label">
							<include>PhotoLabel</include>
							<textcolor>white</textcolor>
						</control>
					</itemlayout>
					<focusedlayout width="136" height="171">
						<include>PhotoThumbCard</include>
						<include>PhotoThumb</include>
						<include>PhotoAlbumCover</include>
						<control type="label">
							<include>PhotoLabel</include>
							<textcolor>orangeLight</textcolor>
						</control>
						<include>SelectedBorder</include>
					</focusedlayout>
				</control>
			</control>
		</control>
	</include>
</includes>
