	<!-- Pre-play Clip -->
	<include name="PreplayClip">
		<control type="group" id="777">
			<posy>-28</posy>
			<visible>Container.PlexContent(Clip) | [Container.PlexContent(Channel) + ListItem.Type(clip)]</visible>
			<include>FadeDialog</include>
			<include>rightSideSlide</include>
			<!-- backgrounds -->
			<control type="largeimage">
				<posy>28</posy>
				<posx>0</posx>
				<width>1280</width>
				<height>720</height>
				<texture>preplay-movie-episode-bg.png</texture>
				<aspectratio>scale</aspectratio>
			</control>
			<!-- Source Icon -->
			<control type="image">
				<posy>146</posy>
				<posx>273</posx>
				<width>204</width>
				<height>204</height>
				<texture>$INFO[ListItem.Art(sourceIcon)]</texture>
				<aspectratio align="right" aligny="center">keep</aspectratio>
				<visible>!IsEmpty(ListItem.Art(sourceIcon))</visible>
			</control>
			<!-- thumb -->
			<control type="largeimage">
				<posy>420</posy>
				<posx>117</posx>
				<width>360</width>
				<height>190</height>
				<texture background="true" diffuse="preplay-poster-big-mask.png">$INFO[ListItem.Art(thumb)]</texture>
				<aspectratio scalediffuse="false" align="center" aligny="top">scale</aspectratio>
				<animation effect="fade" start="0" end="100" time="0" condition="true">Conditional</animation>
			</control>
			<!-- Infos Left -->
			<control type="grouplist">
				<orientation>vertical</orientation>
				<itemgap>10</itemgap>
				<posy>61</posy>
				<posx>560</posx>
				<!-- Title & Watched/Unwatched indicator -->
				<control type="grouplist">
					<width>630</width>
					<height>44</height>
					<align>left</align>
					<usecontrolcoords>true</usecontrolcoords>
					<orientation>horizontal</orientation>
					<itemgap>20</itemgap>
					<control type="label">
						<width min="29" max="570">auto</width>
						<height>44</height>
						<label>$INFO[ListItem.Label]</label>
						<font>Bold-36</font>
						<include>WhiteShadow</include>
						<visible>true</visible>
					</control>
					<control type="image">
						<posy>4</posy>
						<width>30</width>
						<height>30</height>
						<texture>$INFO[ListItem.Overlay]</texture>
					</control>
				</control>
				<!-- Year / Duration / Source -->
				<control type="label">
					<width>630</width>
					<height>30</height>
					<width min="30" max="500">auto</width>
					<height>30</height>
					<label>$INFO[ListItem.Year]$INFO[ListItem.DurationStr, &#xB7; ]$INFO[ListItem.Property(sourceTitle), &#xB7; ]</label>
					<font>Bold-30</font>
					<include>Grey1</include>
				</control>
				<!-- Season, Episode -->
				<control type="label">
					<width>600</width>
					<height>25</height>
					<label>$INFO[ListItem.Date]</label>
					<font>Regular-18</font>
					<include>WhiteShadow</include>
					<visible>true</visible>
				</control>
			</control>
			<!-- Infos Right -->
			<control type="group">
				<posy>61</posy>
				<!-- Star Rating -->
				<control type="image">
					<posy>94</posy>
					<posx>140r</posx>
					<width>100</width>
					<height>20</height>
					<texture>$INFO[ListItem.StarRating]</texture>
					<aspectratio align="right" aligny="center">keep</aspectratio>
					<colordiffuse>orange</colordiffuse>
					<visible>!IsEmpty(ListItem.Property(hasUserRating))</visible>
				</control>
			</control>
			<!-- Director -->
			<control type="grouplist">
				<posy>250</posy>
				<posx>560</posx>
				<orientation>horizontal</orientation>
				<itemgap>10</itemgap>
				<visible>!IsEmpty(ListItem.Director)</visible>
				<control type="label">
					<label>[UPPERCASE]$LOCALIZE[31135][/UPPERCASE]</label>
					<include>LeftInfoFormatGrey</include>
				</control>
				<control type="label">
					<label>$INFO[ListItem.Director]</label>
					<include>LeftInfoFormatWhite</include>
				</control>
			</control>
			<!-- plot -->
			<control type="textbox">
				<label>[B]$INFO[ListItem.Plot][/B]</label>
				<include>PlotFormat</include>
			</control>
			<control type="textbox">
				<include>PlotFormat</include>
				<include>[B]NoPlotDefault[/B]</include>
				<visible>IsEmpty(ListItem.Plot)</visible>
			</control>
			<!-- MediaFlags -->
			<include>MediaFlags</include>
		</control>
	</include>
